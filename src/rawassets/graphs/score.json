{
  "node_list": [
    {
      // Node 0 - Used to trigger the graph on every frame.
      "module": "zooshi",
      "name": "advance_frame"
    },
    {
      // Node 1 - Get the `Player` entity to get the score value attributes.
      "module": "zooshi",
      "name": "player_entity"
    },
    {
      // Node 2 - Get the numerator of the score (PatronsFed).
      "module": "attributes",
      "name": "get_attribute",
      "input_edge_list": [
        {
          "edge_type": "OutputEdgeTarget",
          "edge": {
            "node_index": 0, // `advance_frame`
            "edge_index": 0  // Trigger called on every frame.
          }
        },
        {
          "edge_type": "OutputEdgeTarget",
          "edge": {
            "node_index": 1, // `player_entity`.
            "edge_index": 0  // The `EntityRef` for the player.
          }
        },
        {
          "edge_type": "Int",
          "edge": {
            "value": "fpl.zooshi.AttributeDef.PatronsFed"
          }
        }
      ]
    },
    {
      // Node 3 - Convert the TargetScore (float) to string.
      "module": "string",
      "name": "float_to_string",
      "input_edge_list": [
        {
          "edge_type": "OutputEdgeTarget",
          "edge": {
            "node_index": 2, // `get_attribute`
            "edge_index": 0  // `PatronsFed`
          }
        }
      ]
    },
    {
      // Node 4 - Get the denominator of the score (TargetScore).
      "module": "attributes",
      "name": "get_attribute",
      "input_edge_list": [
        {
          "edge_type": "OutputEdgeTarget",
          "edge": {
            "node_index": 0, // `advance_frame`
            "edge_index": 0  // Trigger called on every frame.
          }
        },
        {
          "edge_type": "OutputEdgeTarget",
          "edge": {
            "node_index": 1, // `player_entity`
            "edge_index": 0  // The `EntityRef` for the player.
          }
        },
        {
          "edge_type": "Int",
          "edge": {
            "value": "fpl.zooshi.AttributeDef.TargetScore"
          }
        }
      ]
    },
    {
      // Node 5 - Convert the TargetScore (float) to string.
      "module": "string",
      "name": "float_to_string",
      "input_edge_list": [
        {
          "edge_type": "OutputEdgeTarget",
          "edge": {
            "node_index": 4, // `get_attribute`
            "edge_index": 0  // `TargetScore`
          }
        }
      ]
    },
    {
      // Node 6 - Concatenation
      "module": "string",
      "name": "concat",
      "input_edge_list": [
        {
          "edge_type": "OutputEdgeTarget",
          "edge": {
            "node_index": 3, // `int_to_string`
            "edge_index": 0  // `PatronsFed` string
          }
        },
        {
          "edge_type": "String",
          "edge": {
            "value": "/"
          }
        }
      ]
    },
    {
      // Node 7 - Concatenation
      "module": "string",
      "name": "concat",
      "input_edge_list": [
        {
          "edge_type": "OutputEdgeTarget",
          "edge": {
            "node_index": 6, // `concat`
            "edge_index": 0  // PatronsFed string + "/"
          }
        },
        {
          "edge_type": "OutputEdgeTarget",
          "edge": {
            "node_index": 5, // `int_to_string`
            "edge_index": 0  // `TargetsScore` string
          }
        }
      ]
    },
    {
      // Node 8 - The score destination entity.
      "module": "entity",
      "name": "graph_entity"
    },
    {
      // Node 9 - Update the `Render3dText` component's string for this entity.
      "module": "ui_string",
      "name": "set_3d_text_string",
      "input_edge_list": [
        {
          "edge_type": "OutputEdgeTarget",
          "edge": {
            "node_index": 8, // `graph_entity`
            "edge_index": 0  // `EntityRef` of the entity to set the 3D text for.
          }
        },
        {
          "edge_type": "OutputEdgeTarget",
          "edge": {
            "node_index": 7, // `concat`
            "edge_index": 0  // `PatronsFed + "/" + TargetScore` string
          }
        }
      ]
    }
  ]
}

